{
  "name": "charting-es6",
  "version": "1.0.0",
  "description": "A boilerplate designed for team library building",
  "main": "index",
  "scripts": {
    "prestart": "babel-node BuildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check test open:src lint:watch",
    "open:src": "babel-node BuildScripts/sourceServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src BuildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress BuildScripts/testSetup.js \"src/**/*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pasquali276/chartCleanup.git"
  },
  "keywords": [
    "es6",
    "hdc",
    "library",
    "charting",
    "chart",
    "barchart"
  ],
  "author": "GPasquali",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pasquali276/chartCleanup/issues"
  },
  "homepage": "https://github.com/pasquali276/chartCleanup#readme",
  "devDependencies": {
    "@types/d3": "^4.9.0",
    "@types/d3-transition": "^1.1.0",
    "@types/lodash": "^4.14.66",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "d3": "^4.9.1",
    "d3-selection": "^1.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "espress": "0.0.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.1.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "0.0.5",
    "style-loader": "^0.17.0",
    "ts-loader": "^2.0.3",
    "tslint": "^5.3.2",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
